e/coder/Documents/contact-keeper/client/src/components/pages/Home.js",["93"],"import React, { useEffect, useContext } from 'react';\nimport Contacts from '../contacts/Contacts';\nimport ContactForm from '../contacts/ContactForm';\nimport ContactState from '../../context/Contact/ContactState';\nimport AuthContext from '../../context/Auth/authContext';\n\nconst Home = () => {\n  const authContext = useContext(AuthContext);\n\n  useEffect(() => {\n    authContext.loadUser();\n    //eslint-disable-next-line\n  }, []);\n  return (\n    <div className='grid-2'>\n      <div>\n        <ContactForm />{' '}\n      </div>\n      <div>\n        <Contacts />\n      </div>\n    </div>\n  );\n};\n\nexport default Home;\n","/home/coder/Documents/contact-keeper/client/src/context/Contact/ContactState.js",[],"/home/coder/Documents/contact-keeper/client/src/context/types.js",[],"/home/coder/Documents/contact-keeper/client/src/context/Contact/contactReducer.js",["94","95","96"],"/home/coder/Documents/contact-keeper/client/src/components/contacts/Contacts.js",["97","98"],"/home/coder/Documents/contact-keeper/client/src/components/contacts/ContactItem.js",[],"/home/coder/Documents/contact-keeper/client/src/components/contacts/ContactForm.js",[],["99","100"],"/home/coder/Documents/contact-keeper/client/src/context/Contact/contactContext.js",[],"/home/coder/Documents/contact-keeper/client/src/context/Auth/authReducer.js",["101","102","103"],"import {\n  REGISTER_SUCCESS,\n  REGISTER_FAIL,\n  USER_LOADED,\n  AUTH_ERROR,\n  LOGIN_SUCCESS,\n  LOGIN_FAIL,\n  LOGOUT,\n  CLEAR_ERRORS,\n} from '../types';\n\nexport default (state, action) => {\n  switch (action.type) {\n    case USER_LOADED:\n      return {\n        ...state,\n        isAuthenticated: true,\n        loading: false,\n        user: action.payload,\n      };\n    case REGISTER_SUCCESS:\n    case LOGIN_SUCCESS:\n      localStorage.setItem('token', action.payload.token);\n      return {\n        ...state,\n        ...action.payload,\n        isAuthenticated: true,\n        loading: false,\n      };\n    case REGISTER_FAIL:\n    case AUTH_ERROR:\n    case LOGIN_FAIL:\n    case LOGOUT:\n      localStorage.removeItem('token');\n      return {\n        ...state,\n        token: null,\n        isAuthenticated: false,\n        loading: false,\n        user: null,\n        error: action.payload,\n      };\n  }\n};\n","/home/coder/Documents/contact-keeper/client/src/context/Auth/AuthState.js",["104","105"],"import { useReducer } from 'react';\nimport axios from 'axios';\nimport AuthContext from './authContext';\nimport authReducer from './authReducer';\nimport setAuthToken from '../../utils/setAuthToken';\nimport {\n  REGISTER_SUCCESS,\n  REGISTER_FAIL,\n  USER_LOADED,\n  AUTH_ERROR,\n  LOGIN_SUCCESS,\n  LOGIN_FAIL,\n  LOGOUT,\n  CLEAR_ERRORS,\n} from '../types';\n\nconst AuthState = (props) => {\n  const initialState = {\n    token: localStorage.getItem('token'),\n    isAuthenticated: null,\n    loading: true,\n    user: null,\n    error: null,\n  };\n\n  const [state, dispatch] = useReducer(authReducer, initialState);\n\n  //Load User\n  const loadUser = async () => {\n    //@todo - load token into global headers\n\n    if (localStorage.token) {\n      setAuthToken(localStorage.token);\n    }\n    try {\n      const res = await axios.get('/api/auth');\n\n      dispatch({\n        type: USER_LOADED,\n        payload: res.data,\n      });\n    } catch (err) {}\n  };\n  //Register User\n  const register = async (formData) => {\n    try {\n      const res = await axios.post('/api/users', formData, {\n        headers: {\n          'Content-Type': 'application/json',\n        },\n      });\n\n      dispatch({\n        type: REGISTER_SUCCESS,\n        payload: res.data,\n      });\n      loadUser();\n    } catch (err) {\n      dispatch({ type: REGISTER_FAIL, payload: err.response.data });\n    }\n  };\n  //Login User\n  const login = async (formData) => {\n    try {\n      const res = await axios.post('/api/auth', formData, {\n        headers: {\n          'Content-Type': 'application/json',\n        },\n      });\n\n      dispatch({\n        type: LOGIN_SUCCESS,\n        payload: res.data,\n      });\n      loadUser();\n    } catch (err) {\n      dispatch({ type: LOGIN_FAIL, payload: err.response.data.msg });\n    }\n  };\n  //Logout\n  const logout = () => dispatch({ type: LOGOUT });\n\n  //Clear errors\n  const clearErrors = () => console.log('loaduser');\n\n  return (\n    <AuthContext.Provider\n      value={{\n        token: state.token,\n        isAuthenticated: state.isAuthenticated,\n        loading: state.loading,\n        user: state.user,\n        error: state.error,\n        register,\n        loadUser,\n        login,\n        logout,\n        clearErrors,\n      }}\n    >\n      {props.children}\n    </AuthContext.Provider>\n  );\n};\n\nexport default AuthState;\n","/home/coder/Documents/contact-keeper/client/src/context/Auth/authContext.js",[],["106","107"],"/home/coder/Documents/contact-keeper/client/src/components/auth/Register.js",[],"/home/coder/Documents/contact-keeper/client/src/components/auth/Login.js",[],"/home/coder/Documents/contact-keeper/client/src/utils/setAuthToken.js",[],"/home/coder/Documents/contact-keeper/client/src/components/routing/PrivateRoute.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"108","replacedBy":"112"},{"ruleId":"110","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":4,"column":8,"nodeType":"116","messageId":"117","endLine":4,"endColumn":20},{"ruleId":"114","severity":1,"message":"118","line":9,"column":3,"nodeType":"116","messageId":"117","endLine":9,"endColumn":18},{"ruleId":"114","severity":1,"message":"119","line":10,"column":3,"nodeType":"116","messageId":"117","endLine":10,"endColumn":15},{"ruleId":"120","severity":1,"message":"121","line":14,"column":1,"nodeType":"122","endLine":69,"endColumn":3},{"ruleId":"114","severity":1,"message":"115","line":3